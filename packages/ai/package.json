{
  "name": "@ui5/webcomponents-ai-react",
  "version": "2.15.3",
  "description": "React wrapper for `@ui5/webcomponents-ai` web components.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./dist": "./dist/index.js",
    "./dist/*": "./dist/*",
    "./dist/*.js": "./dist/*.js",
    "./Button": {
      "types": "./dist/components/Button/index.d.ts",
      "default": "./dist/components/Button/index.js"
    },
    "./ButtonState": {
      "types": "./dist/components/ButtonState/index.d.ts",
      "default": "./dist/components/ButtonState/index.js"
    },
    "./PromptInput": {
      "types": "./dist/components/PromptInput/index.d.ts",
      "default": "./dist/components/PromptInput/index.js"
    }
  },
  "homepage": "https://ui5.github.io/webcomponents-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/UI5/webcomponents-react.git",
    "directory": "packages/ai"
  },
  "author": "SAP SE (https://www.sap.com)",
  "license": "Apache-2.0",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build:version-info": "node ../../scripts/generate-version-info.js"
  },
  "dependencies": {
    "@ui5/webcomponents-react-base": "workspace:~"
  },
  "peerDependencies": {
    "@ui5/webcomponents-ai": "~2.15.0",
    "react": "^18 || ^19"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "NOTICE.txt"
  ]
}
